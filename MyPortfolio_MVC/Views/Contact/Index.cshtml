@model List<MyPortfolio_MVC.Models.TblContact>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}

<h2>İletişim</h2>

<table class="table table-bordered table-hover">

    <tr>
        <th>Telefon Numarası</th>
        <th>Email</th>
        <th>Güncelle</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>@item.Phone</td>
            <td>@item.Email</td>
            <td>
                <a href="/Contact/UpdateContact/@item.ContactId" class="btn btn-outline-info">Güncelle</a>
            </td>

        </tr>
    }

</table>